        "@fortawesome/vue-fontawesome": "^0.1.9",
        "axios": "^0.19.2",
        "bootstrap": "^4.4.1",
        "bootstrap-vue": "^2.9.0",
        "collect.js": "^4.20.3",
        "cropperjs": "^1.5.6",
        "fs": "0.0.1-security",
        "highlight.js": "^9.18.1",
        "i18n": "^0.8.5",
        "i18n-js": "^3.5.1",
        "jquery": "^3.2",
        "lodash": "^4.17.15",
        "marked": "^1.1.0",
        "placeholder-loading": "^0.2.6",
        "popper.js": "^1.16.1",
