{
    "name": "fjord",
    "description": "Fjord is a multilanguage admin-panel/cms scaffolding package that helps you creating CRUD in seconds via Artisan-commands. It also lets you manage the \"static\" content of each of your websites pages (Headlines, Text, Images), as well repetitive mixed contents you define. Generating content as well as passing it to your views and retrieving it is super simple.",
    "version": "2.0.0",
    "main": "./resources/js/index.js",
    "scripts": {
        "test": "./node_modules/.bin/jest .*.spec.js"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            "^.+\\.(js|jsx)?$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "vue-jest"
        },
        "moduleNameMapper": {
            "fjord-test": "<rootDir>/tests/javascript/bootstrap.js",
            "@fj-js(.*)$": "<rootDir>/resources/js/$1",
            "@fj-sass(.*)$": "<rootDir>/resources/sass/$1"
        }
    },
    "config": {
        "exportAliases": {
            "mix": "./resources/js/mix.js"
        }
    },
    "keywords": [
        "fjord",
        "cms",
        "fjord-cms"
    ],
    "author": [
        "Jannes Behrens",
        "Lennart Carstens-Behrens"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "*",
        "@babel/preset-env": "*",
        "@vue/server-test-utils": "^1.0.3",
        "@vue/test-utils": "^1.0.3",
        "babel-core": "^6.26.3",
        "babel-jest": "^26.0.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-require-context-hook": "^1.0.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-require-context": "^0.1.1",
        "babel-preset-env": "^1.7.0",
        "cross-env": "^7.0.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^26.0.1",
        "jest-serializer-vue": "^2.0.2",
        "karma": "^5.0.9",
        "node-sass-glob-importer": "^5.3.2",
        "resolve-url-loader": "^3.1.0",
        "sass-loader": "^8.0.0",
        "vue-jest": "^3.0.5",
        "vue-template-compiler": "^2.6.11",
        "vuepress": "^1.2.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.27",
        "@fortawesome/free-brands-svg-icons": "^5.12.1",
